pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
--	this program works with
--	memory management to create
--	bigger floats/ints

#include memory.lua
#include util.lua
#include register.lua
#include inops.lua
#include flops.lua

function _init()
	word_size = 8 -- in bytes

	moffset = 0x4300 --	offset to user memory
	mend = 0x5e00 -- end of user memory
	mtotal = mend - moffset --	total memory length (bytes)

	clear_mem(0x4300, 0x1B00) -- clears user memory
	
	init_memory(word_size)

end

function _update()
end

function _draw()
	cls(0)
	draw_memory()
	print("")
	
	--	prints some of bit-map
	for i=0,3 do
		print(print_peek(moffset+i))
	end
end

